{
    "name": "varejointeligente/nfse-etransparencia",
    "type": "library",
    "description": "Package Laravel para geração de NFSE padrão Conan/eTransparencia.",
    "keywords": ["nfe","nfse","nfephp"],
    "homepage": "https://github.com/varejointeligente/nfse-etransparencia",
    "license": ["LGPL-3.0-or-later", "GPL-3.0-or-later", "MIT"],
    "authors": [
        {
            "name": "Cleiton Waldemar Ribeiro",
            "email": "big.dark@gmail.com",
            "homepage": "http://www.varejointeligente.tech",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">= 7.1",
        "nfephp-org/sped-common" : "^5.0",
        "justinrainbow/json-schema": "^5.2",
        "ext-zlib": "*",
        "ext-dom": "*",
        "ext-openssl": "*",
        "ext-json": "*",
        "ext-simplexml": "*",
        "ext-libxml": "*"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^2.8",
        "phpunit/phpunit": "^5.7",
        "scrutinizer/ocular": "^1.3",
        "phpmd/phpmd": "dev-master",
        "sebastian/phpcpd": "^3.0",
        "phpstan/phpstan": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
          "NFePHP\\NFSeTrans\\": "src/"
        } 
    },
    "autoload-dev": {
        "psr-4": {
            "NFePHP\\NFSeTrans\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit -c phpunit.xml.dist",
        "phpcbf": "vendor/bin/phpcbf --standard=psr2 src; vendor/bin/phpcbf --standard=psr2 tests",
        "phpcs": "vendor/bin/phpcs --standard=psr2 src; vendor/bin/phpcs --standard=psr2 tests",
        "phpstan": "vendor/bin/phpstan analyse src/ --level 7"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
